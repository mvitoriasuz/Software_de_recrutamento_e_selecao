<%- include('./partials/header.ejs') %>
<%- include('./partials/navbar.ejs') %>

<header id="header" class="ex-2-header">
  <div class="container">
      <div class="row">
          <div class="col-lg-12">
              <h1>Cadastro de Vagas</h1>
              <!-- Sign Up Form -->
              <div class="form-container">
                  <form id="logInForm" data-toggle="validator" data-focus="false">
                      <div class="form-group">
                          <input type="email" class="form-control-input" name="email" required>
                          <label class="label-control" for="lemail">Email</label>
                          <div class="help-block with-errors"></div>
                      </div>
                      <div class="form-group">
                          <input type="text" class="form-control-input" name="password" required>
                          <label class="label-control" for="password">Password</label>
                          <div class="help-block with-errors"></div>
                      </div>
                      <div class="form-group">
                        <input type="text" class="form-control-input" name="cnpj" required>
                        <label class="label-control" for="cnpj">CNPJ</label>
                        <div class="help-block with-errors"></div>
                    </div>
                    <div class="form-group">
                      <input type="text" class="form-control-input" name="cargo" required>
                      <label class="label-control" for="cargo">Cargo</label>
                      <div class="help-block with-errors"></div>
                  </div>
                      <div class="form-group">
                          <button type="submit" class="form-control-submit-button">Submit</button>
                      </div>
                      <div class="form-message">
                          <div id="lmsgSubmit" class="h3 text-center hidden"></div>
                      </div>
                  </form>
              </div> <!-- end of form container -->
              <!-- end of sign up form -->
              <br>
          </div> <!-- end of col --></div>
      </div> <!-- end of row -->
  </div> <!-- end of container -->
</header> <!-- end of ex-header -->
